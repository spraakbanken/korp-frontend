  ul#cog_menu.dropdown-menu.popper_menu
    li
      a#about(ng-click="show_about = 'about'", style="cursor: pointer;") {{'about' | loc:lang}}
    li
      a#docs.follow_link(href='http://spraakbanken.gu.se/swe/forskning/infrastruktur/korp/anvandarhandledning', target='_blank', rel='localize[docs]')
    li#korplink
      a.follow_link(href='/korp', rel='localize[korp]')
    li#korplablink
      a.follow_link(href='/korplabb', rel='localize[korp_lab]')
    li#import
      a.follow_link(href='https://spraakbanken.gu.se/sparv', rel='localize[import_chain]', target='_blank')
    li#login
      a(href='javascript:', rel='localize[log_in]')
  #news_area
    div(news-desk="", header="'newsdesk-header'", storage="'korp_last_read_newsitem'")
  #log_out.link
    span 
      | {{'log_out' | loc:lang}}   
    span.usrname 
  #languages
    a(href='javascript:', data-mode='sv', ng-click="lang = 'sv'") {{'swedish' | loc:lang}}
    a(href='javascript:', data-mode='en', ng-click="lang = 'en'") {{'english' | loc:lang}}
  #cite
    a(href="javascript:", ng-click="citeClick()") {{"about_cite_header" | loc:lang}}

  .clearfix
#header_left
  a#logo
    img(src='img/korplogo.png')
